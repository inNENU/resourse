[{"time":8.156,"text":"成都"},{"time":11.9,"text":"原唱: 赵雷"},{"time":14.828,"text":"演唱: 翟欣奕"},{"time":17.708,"text":"歌词改编: 荆鹏"},{"time":21.572,"text":"歌词: Mr.Hope"},{"time":24.356},{"time":25.892,"text":"让我掉下眼泪的"},{"time":29.948,"text":"不止四年停留"},{"time":34.052,"text":"让我依依不舍的"},{"time":38.132,"text":"不止静湖钟楼"},{"time":42.38,"text":"明天我会去挥手"},{"time":46.436,"text":"'5268’门口"},{"time":50.564,"text":"让我感到为难的"},{"time":54.716,"text":"是离别的忧愁"},{"time":58.7},{"time":60.932,"text":"相识总是在九月"},{"time":65.252,"text":"地院前那个路口"},{"time":69.164,"text":"盛夏学林的松鼠"},{"time":73.244,"text":"匆匆跑在你前头"},{"time":77.348,"text":"在那逸夫楼的教室里"},{"time":81.596,"text":"我多想认识你"},{"time":85.58,"text":"东师"},{"time":87.428,"text":"忘不掉的只有你"},{"time":93.284},{"time":94.196,"text":"和我在静湖的亭边走一走"},{"time":102.236,"text":"直到所有的荷都闭合了也不停留"},{"time":110.42,"text":"你会挽着我的衣袖"},{"time":114.548,"text":"去南门提水果一兜"},{"time":118.772,"text":"走到行知路的尽头"},{"time":122.9,"text":"坐在图书馆的门口"},{"time":128.996},{"time":162.548,"text":"分别总是在六月"},{"time":166.436,"text":"思念很快溜走"},{"time":170.612,"text":"盛夏柳园的垂柳"},{"time":174.644,"text":"亲吻着我额头"},{"time":178.844,"text":"在中快二食堂美味里"},{"time":182.972,"text":"我永远不会腻"},{"time":187.22,"text":"东师 带不走的 只有你"},{"time":195.524,"text":"和我在旭日广场上走一走"},{"time":203.756,"text":"直到雕塑的灯都熄灭了也不停留"},{"time":211.892,"text":"你会倚在我的肩头"},{"time":216.14,"text":"说很久都停不住口"},{"time":220.364,"text":"走到桂林路的摊头"},{"time":224.252,"text":"忘掉一天烦恼忧愁"},{"time":228.284,"text":"和我在校训石旁合影停留"},{"time":236.636,"text":"直到勤奋创新为人师表烙进心头"},{"time":244.964,"text":"你会和我手牵着手"},{"time":249.164,"text":"远方有你哼唱在口"},{"time":253.244,"text":"走到地铁站的门口"},{"time":257.42,"text":"再和我师大挥挥手"},{"time":263.972}]