{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://mp.innenu.com/.vscode/components.schema.json",
  "$defs": {
    "service-status": {
      "description": "服务状态",
      "oneOf": [{ "const": "local" }, { "const": "online" }]
    },
    "image-link": {
      "description": "图片地址",
      "type": "string",
      "pattern": "^(?:https?:\\/\\/|\\$img/).*\\.(?:jpe?g|png|gif)$"
    },
    "env": {
      "type": "array",
      "items": {
        "enum": ["app", "qq", "wx", "web"]
      }
    },
    "style-object": {
      "type": "object",
      "patternProperties": {
        "^.+$": {
          "type": "string"
        }
      }
    },
    "location": {
      "type": "object",
      "properties": {
        "longitude": {
          "description": "经度",
          "type": "number"
        },
        "latitude": {
          "description": "纬度",
          "type": "number"
        }
      },
      "required": ["longitude", "latitude"],
      "additionalProperties": false
    },
    "point": {
      "type": "object",
      "properties": {
        "longitude": {
          "description": "经度",
          "type": "number"
        },
        "latitude": {
          "description": "纬度",
          "type": "number"
        },
        "name": {
          "description": "点位名称",
          "type": "string"
        },
        "detail": {
          "description": "点位详情文字",
          "type": "string"
        },
        "path": {
          "description": "地点详情路径",
          "type": "string"
        }
      },
      "required": ["longitude", "latitude"],
      "additionalProperties": false
    },
    "base-list-item": {
      "type": "object",
      "properties": {
        "text": {
          "description": "列表项目文字",
          "type": "string"
        },
        "icon": {
          "description": "列表项目图标",
          "type": "string"
        },
        "desc": {
          "description": "列表项目描述",
          "type": "string"
        },
        "path": {
          "description": "列表项目路径",
          "type": "string"
        },
        "url": {
          "description": "列表项目地址",
          "type": "string"
        },
        "env": {
          "$ref": "#/$defs/env"
        }
      },
      "required": ["text"],
      "default": { "text": "", "path": "" },
      "additionalProperties": false
    },
    "navigator-list-item": {
      "type": "object",
      "properties": {
        "type": {
          "const": "navigator"
        },
        "text": {
          "description": "列表项目文字",
          "type": "string"
        },
        "icon": {
          "description": "列表项目图标",
          "type": "string"
        },
        "desc": {
          "description": "列表项目描述",
          "type": "string"
        },
        "openType": {
          "description": "小程序提供的开放能力",
          "type": "string"
        },
        "target": {
          "description": "跳转目标",
          "type": "string"
        },
        "url": {
          "description": "列表项目地址",
          "type": "string"
        }
      },
      "required": ["text"],
      "default": { "type": "navigator", "text": "" },
      "additionalProperties": false
    },
    "switch-list-item": {
      "type": "object",
      "properties": {
        "type": {
          "const": "switch"
        },
        "text": {
          "description": "列表项目文字",
          "type": "string"
        },
        "icon": {
          "description": "列表项目图标",
          "type": "string"
        },
        "desc": {
          "description": "列表项目描述",
          "type": "string"
        },
        "key": {
          "description": "所控变量在 storage 中的 key 值",
          "type": "string"
        },
        "handler": {
          "description": "开关对应的函数名称",
          "type": "string"
        },
        "color": {
          "description": "开关颜色",
          "type": "string"
        }
      },
      "required": ["text", "key"],
      "default": { "type": "switch", "text": "", "key": "" },
      "additionalProperties": false
    },
    "picker-list-item": {
      "type": "object",
      "properties": {
        "type": {
          "const": "picker"
        },
        "text": {
          "description": "列表项目文字",
          "type": "string"
        },
        "icon": {
          "description": "列表项目图标",
          "type": "string"
        },
        "desc": {
          "description": "列表项目描述",
          "type": "string"
        },
        "select": {
          "description": "选择器中包含的值",
          "type": "array"
        },
        "key": {
          "description": "所控变量在 storage 中的 key 值",
          "type": "string"
        },
        "handler": {
          "description": "开关对应的函数名称",
          "type": "string"
        },
        "single": {
          "description": "是否为单列选择器",
          "type": "boolean"
        },
        "inlay": {
          "description": "是否为嵌入式 picker",
          "type": "boolean"
        }
      },
      "required": ["text", "select", "key"],
      "default": { "text": "", "select": [], "key": "" },
      "additionalProperties": false
    },
    "slider-list-item": {
      "type": "object",
      "properties": {
        "type": {
          "const": "slider"
        },
        "text": {
          "description": "列表项目文字",
          "type": "string"
        },
        "icon": {
          "description": "列表项目图标",
          "type": "string"
        },
        "desc": {
          "description": "列表项目描述",
          "type": "string"
        },
        "key": {
          "description": "所控变量在 storage 中的 key 值",
          "type": "string"
        },
        "handler": {
          "description": "开关对应的函数名称",
          "type": "string"
        },
        "min": {
          "description": "最小值",
          "type": "number"
        },
        "max": {
          "description": "最大值",
          "type": "number"
        },
        "step": {
          "description": "步长",
          "type": "number"
        }
      },
      "required": ["text", "key"],
      "default": { "type": "slider", "text": "", "handler": "" },
      "additionalProperties": false
    },
    "button-list-item": {
      "type": "object",
      "properties": {
        "type": {
          "const": "button"
        },
        "text": {
          "description": "列表项目文字",
          "type": "string"
        },
        "icon": {
          "description": "列表项目图标",
          "type": "string"
        },
        "desc": {
          "description": "列表项目描述",
          "type": "string"
        },
        "handler": {
          "description": "开关对应的函数名称",
          "type": "string"
        },
        "openType": {
          "description": "开放能力",
          "type": "string",
          "enum": [
            "share",
            "openSetting",
            "openGroupProfile",
            "openGuildProfile",
            "contact",
            "addFriend",
            "addGroupApp",
            "addToFavorites",
            "getPhoneNumber",
            "getUserInfo",
            "feedback",
            "chooseAvatar"
          ]
        },
        "openId": {
          "description": "用户 openId",
          "type": "string"
        },
        "groupId": {
          "description": "群 ID",
          "type": "string"
        },
        "guildId": {
          "description": "频道 ID",
          "type": "string"
        },
        "disabled": {
          "description": "是否禁用按钮",
          "type": "boolean"
        }
      },
      "required": ["text"],
      "default": { "type": "button", "text": "", "handler": "" },
      "additionalProperties": false
    },
    "grid-item": {
      "type": "object",
      "properties": {
        "text": {
          "description": "网格项目文字",
          "type": "string"
        },
        "icon": {
          "description": "网格项目图标",
          "type": "string"
        },
        "color": {
          "description": "网格颜色",
          "type": "string",
          "enum": [
            "red",
            "blue",
            "purple",
            "orange",
            "grey",
            "mauve",
            "olive",
            "cyan"
          ]
        },
        "name": {
          "description": "网格项目英文名称",
          "type": "string"
        },
        "path": {
          "description": "网格项目路径",
          "type": "string"
        },
        "url": {
          "description": "网格项目地址",
          "type": "string"
        },
        "env": {
          "$ref": "#/$defs/env"
        }
      },
      "required": ["text", "icon", "color", "name"],
      "default": {
        "text": "",
        "icon": "",
        "color": "",
        "name": "",
        "path": ""
      },
      "additionalProperties": false
    },
    "title-component": {
      "$comment": "https://innenu.com/contributing/tag-list.html#title",
      "description": "标题组件",
      "type": "object",
      "properties": {
        "tag": {
          "description": "标签名称",
          "const": "title"
        },
        "text": {
          "description": "标题文字",
          "type": "string"
        },
        "style": {
          "description": "标题样式",
          "oneOf": [{ "type": "string" }, { "$ref": "#/$defs/style-object" }]
        },
        "env": {
          "$ref": "#/$defs/env"
        }
      },
      "required": ["tag", "text"],
      "default": { "tag": "title", "text": "" },
      "additionalProperties": false
    },
    "table-component": {
      "$comment": "https://innenu.com/contributing/tag-list.html#table",
      "description": "表格组件",
      "type": "object",
      "properties": {
        "tag": {
          "description": "标签名称",
          "const": "table"
        },
        "caption": {
          "description": "表格标题",
          "type": "string"
        },
        "header": {
          "description": "表格表头",
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "body": {
          "description": "表格内容",
          "type": "array",
          "items": {
            "description": "表格每行内容",
            "type": "array",
            "items": {
              "description": "表格单元文字",
              "type": "string"
            }
          }
        },
        "env": {
          "$ref": "#/$defs/env"
        }
      },
      "required": ["header", "body"],
      "default": { "tag": "table", "header": [], "body": [] },
      "additionalProperties": false
    },
    "text-component": {
      "$comment": "https://innenu.com/contributing/tag-list.html#text",
      "description": "标题组件",
      "type": "object",
      "properties": {
        "tag": {
          "description": "标签名称",
          "enum": ["text", "p", "ol", "ul"]
        },
        "heading": {
          "description": "文字块标题",
          "oneOf": [{ "type": "string" }, { "const": true }]
        },
        "text": {
          "description": "文字内容",
          "oneOf": [
            { "type": "string" },
            { "type": "array", "items": { "type": "string" } }
          ]
        },
        "type": {
          "description": "文字块类型",
          "enum": ["tip", "warning", "danger", "info", "note"]
        },
        "path": {
          "description": "跳转路径",
          "type": "string"
        },
        "url": {
          "description": "跳转页面",
          "type": "string"
        },
        "align": {
          "description": "文字块对齐",
          "enum": ["left", "right", "center", "justify"]
        },
        "style": {
          "description": "文字样式",
          "oneOf": [{ "type": "string" }, { "$ref": "#/$defs/style-object" }]
        },
        "env": {
          "$ref": "#/$defs/env"
        }
      },
      "default": { "tag": "text", "text": [""] },
      "required": ["tag"],
      "additionalProperties": false
    },
    "list-component": {
      "$comment": "https://innenu.com/contributing/tag-list.html#list",
      "description": "列表组件",
      "type": "object",
      "properties": {
        "tag": {
          "description": "标签名称",
          "const": "list"
        },
        "header": {
          "description": "列表标题",
          "type": ["string", "boolean"],
          "oneOf": [{ "type": "string" }, { "const": false }]
        },
        "items": {
          "description": "列表项目",
          "type": "array",
          "items": {
            "$ref": "#/$defs/base-list-item"
          }
        },
        "footer": {
          "description": "列表页脚",
          "type": "string"
        },
        "env": {
          "$ref": "#/$defs/env"
        }
      },
      "required": ["tag", "items"],
      "default": { "tag": "list", "items": [] },
      "additionalProperties": false
    },
    "img-component": {
      "$comment": "https://innenu.com/contributing/tag-list.html#img",
      "description": "图片组件",
      "type": "object",
      "properties": {
        "tag": {
          "description": "标签名称",
          "const": "img"
        },
        "src": {
          "$ref": "#/$defs/image-link"
        },
        "res": {
          "$ref": "#/$defs/image-link"
        },
        "desc": {
          "description": "图片描述",
          "type": "string"
        },
        "watermark": {
          "description": "图片水印",
          "type": "boolean"
        },
        "imgMode": {
          "description": "图片填充模式",
          "type": "string",
          "enum": [
            "widthFix",
            "scaleToFill",
            "aspectFit",
            "aspectFill",
            "top",
            "bottom",
            "center",
            "left",
            "right",
            "top left",
            "top right",
            "bottom left",
            "bottom right"
          ]
        },
        "env": {
          "$ref": "#/$defs/env"
        }
      },
      "required": ["tag", "src"],
      "default": { "tag": "img", "src": "" },
      "additionalProperties": false
    },
    "doc-component": {
      "$comment": "https://innenu.com/contributing/tag-list.html#doc",
      "description": "文档组件",
      "type": "object",
      "properties": {
        "tag": {
          "description": "标签名称",
          "const": "doc"
        },
        "name": {
          "description": "文档名称",
          "type": "string"
        },
        "url": {
          "description": "文档地址",
          "type": "string",
          "pattern": "^(https?:\\/\\/|\\$file/|\\$img/).*\\.(?:docx?|pptx?|xlsx?|pdf|jpe?g|png|gif)$"
        },
        "header": {
          "description": "组件标题",
          "type": "string"
        },
        "downloadable": {
          "type": "boolean",
          "default": false
        },
        "env": {
          "$ref": "#/$defs/env"
        }
      },
      "required": ["tag", "name", "url"],
      "default": { "tag": "doc", "name": "", "url": "" },
      "additionalProperties": false
    },
    "phone-component": {
      "$comment": "https://innenu.com/contributing/tag-list.html#phone",
      "description": "电话组件",
      "type": "object",
      "properties": {
        "tag": {
          "description": "标签名称",
          "const": "phone"
        },
        "num": {
          "description": "电话号码",
          "type": ["string", "number"],
          "oneOf": [
            { "type": "number" },
            { "type": "string", "pattern": "^(?:0\\d{2,3}-)?\\d+$" }
          ]
        },
        "header": {
          "description": "组件标题",
          "type": "string"
        },
        "fName": {
          "description": "联系人的名称",
          "type": "string"
        },
        "lName": {
          "description": "联系人的姓氏",
          "type": "string"
        },
        "org": {
          "description": "联系人所在公司",
          "type": "string"
        },
        "remark": {
          "description": "联系人备注",
          "type": "string"
        },
        "workNum": {
          "description": "联系人的工作电话",
          "type": ["string", "number"],
          "oneOf": [
            { "type": "number" },
            { "type": "string", "pattern": "^(?:0\\d{2,3}-)?\\d+$" }
          ]
        },
        "nick": {
          "description": "联系人昵称",
          "type": "string"
        },
        "wechat": {
          "description": "联系人微信号",
          "type": "string"
        },
        "province": {
          "description": "联系人的省份地址",
          "type": "string"
        },
        "city": {
          "description": "联系人的城市地址",
          "type": "string"
        },
        "street": {
          "description": "联系人的街道地址",
          "type": "string"
        },
        "postCode": {
          "type": "number",
          "minLength": 6,
          "maxLength": 6
        },
        "title": {
          "description": "联系人的职位",
          "type": "string"
        },
        "hostNum": {
          "description": "联系人的公司电话",
          "type": ["string", "number"],
          "oneOf": [
            { "type": "number" },
            { "type": "string", "pattern": "^(?:0\\d{2,3}-)?\\d+$" }
          ]
        },
        "site": {
          "description": "联系人的网站",
          "type": "string",
          "pattern": "^https?:\\/\\/"
        },
        "mail": {
          "description": "联系人的电子邮件",
          "type": "string",
          "pattern": "^\\w(?:[\\w._-]*\\w)?@(?:\\w(?:[\\w-]*\\w)?\\.)*\\w+$"
        },
        "homeNum": {
          "description": "联系人的住宅电话",
          "type": ["string", "number"],
          "oneOf": [
            { "type": "number" },
            { "type": "string", "pattern": "^(?:0\\d{2,3}-)?\\d+$" }
          ]
        },
        "avatar": {
          "description": "联系人头像图片路径(仅限本地路径)",
          "type": "string",
          "pattern": "^\\/.*\\.{jpe?g,png}$"
        },
        "env": {
          "$ref": "#/$defs/env"
        }
      },
      "required": ["tag", "num", "fName"],
      "default": { "tag": "phone", "number": "" },
      "additionalProperties": false
    },
    "action-component": {
      "$comment": "https://innenu.com/contributing/tag-list.html#action",
      "description": "动作组件",
      "type": "object",
      "properties": {
        "tag": {
          "description": "标签名称",
          "const": "action"
        },
        "content": {
          "description": "动作内容",
          "type": "string"
        },
        "header": {
          "description": "组件标题",
          "type": "string"
        },
        "env": {
          "$ref": "#/$defs/env"
        }
      },
      "required": ["tag", "content"],
      "default": { "tag": "action", "content": "" },
      "additionalProperties": false
    },
    "account-component": {
      "$comment": "https://innenu.com/contributing/tag-list.html#account",
      "description": "账户组件",
      "type": "object",
      "properties": {
        "tag": {
          "description": "标签名称",
          "const": "account"
        },
        "name": {
          "description": "主体名称",
          "type": "string"
        },
        "logo": {
          "description": "头像图标在线地址",
          "type": "string"
        },
        "detail": {
          "description": "主体的全称",
          "type": "string"
        },
        "desc": {
          "description": "主体描述",
          "type": "string"
        },
        "qq": {
          "description": "主体的 QQ 号",
          "type": "number"
        },
        "qqid": {
          "description": "主体的 QQ Openid",
          "type": "string"
        },
        "qqcode": {
          "description": "主体的 QQ 二维码地址",
          "type": "string"
        },
        "wxid": {
          "description": "主体的微信公众号 ID",
          "type": "string"
        },
        "wxcode": {
          "description": "主体的微信二维码地址",
          "type": "string"
        },
        "account": {
          "description": "主体的微信公众号配置文件 ID",
          "type": "string"
        },
        "location": {
          "description": "主体的地址",
          "type": "object",
          "$ref": "#/$defs/location"
        },
        "mail": {
          "description": "主体的电子邮件",
          "type": "string",
          "pattern": "^\\w(?:[\\w._-]*\\w)?@(?:\\w(?:[\\w-]*\\w)?\\.)*\\w+$"
        },
        "site": {
          "description": "主题的网站",
          "type": "string",
          "pattern": "^https?:\\/\\/"
        },
        "env": {
          "$ref": "#/$defs/env"
        }
      },
      "required": ["tag", "name", "logo"],
      "default": { "tag": "account", "name": "", "logo": "" },
      "additionalProperties": false
    },
    "location-component": {
      "$comment": "https://innenu.com/contributing/tag-list.html#location",
      "description": "地点组件",
      "type": "object",
      "properties": {
        "tag": {
          "description": "标签名称",
          "const": "location"
        },
        "title": {
          "description": "位置展示的内容",
          "type": "string"
        },
        "points": {
          "description": "地点",
          "type": "array",
          "items": {
            "$ref": "#/$defs/point"
          }
        },
        "navigate": {
          "description": "是否允许导航，仅在企业主体微信小程序上可用",
          "type": "boolean"
        },
        "env": {
          "$ref": "#/$defs/env"
        }
      },
      "required": ["tag", "title", "points"],
      "default": { "tag": "location", "content": "" },
      "additionalProperties": false
    },
    "carousel-component": {
      "$comment": "https://innenu.com/contributing/tag-list.html#carousel",
      "description": "地点组件",
      "type": "object",
      "properties": {
        "tag": {
          "description": "标签名称",
          "const": "carousel"
        },
        "images": {
          "description": "图片",
          "type": "array",
          "items": {
            "$ref": "#/$defs/image-link"
          }
        },
        "fill": {
          "description": "组件是否填满屏幕宽度",
          "type": "boolean"
        },
        "indicatorDots": {
          "description": "是否显示面板指示点",
